{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "dev": "npx nodemon",
    "build-client": "cd ../client && npm run build && cd ../server",
    "delete-public": "rmdir /s /q public && mkdir public",
    "copy-client": "xcopy /s \"../client/dist\" public",
    "build": "npm run build-client && npm run delete-public && npm run copy-client"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "deta": "^1.1.0",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
